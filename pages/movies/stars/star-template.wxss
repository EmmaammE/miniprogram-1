.star-container {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.star-container .stars {
    display: flex;
    flex-direction: row;
    height: 17rpx;
    margin-right: 24rpx;
}

.star-container .stars image {
    width: 17rpx;
    height: 17rpx;
    padding-left: 3rpx;
}

.stars .star-score {
    color: #555;
}